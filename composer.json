{
  "name": "bizley/quill",
  "description": "Quill editor implementation for Yii 2.",
  "type": "yii2-extension",
  "keywords": ["yii2","quill","wysiwyg","widget","editor"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Pawel Bizley Brzozowski",
      "email": "pawel@positive.codes"
    },
    {
      "name": "Simon Karlen",
      "email": "simi.albi@outlook.com"
    }
  ],
  "support": {
    "source": "https://github.com/bizley/yii2-quill"
  },
  "require": {
    "php": "^7.0",
    "yiisoft/yii2": "^2.0.0",
    "npm-asset/quill": "^1.3.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "yiisoft/yii2-coding-standards": "^2.0.0",
    "npm-asset/highlightjs": "^9.16.2",
    "npm-asset/katex": "^0.11.1"
  },
  "suggest": {
    "npm-asset/highlightjs": "To use syntax highlighting",
    "npm-asset/katex": "To use formular module"
  },
  "autoload": {
    "psr-4": {
      "bizley\\quill\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
